{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb","prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/react-in-jsx-scope":"off",
        "react/prop-types":"off",
        "react/destructuring-assignment":"off",
        "spaced-comment":"off"
    }
}
