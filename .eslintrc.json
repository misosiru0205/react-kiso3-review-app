{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb","prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "spaced-comment":"off",
        "import/prefer-default-export" : "off",
        "react/react-in-jsx-scope" : "off",
        "import/no-duplicates":"off",
        "no-use-before-define" : "off"
    }
}
